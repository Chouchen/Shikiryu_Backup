{
  "name": "shikiryu/backup",
  "description": "Backup script for limited shared hosting",
  "version": "0.0.1",
  "authors": [
    {
      "name": "Shikiryu",
      "email": "backup@desmidt.fr",
      "homepage": "https://shikiryu.com",
      "role": "Developer"
    }
  ],
  "require": {
    "phpseclib/phpseclib": ">=1.0.0",
    "ext-fileinfo": "*",
    "ext-zip": "*",
    "ext-ftp": "*",
    "ext-pdo": "*",
    "ext-mbstring": "*",
    "ext-json": "*",
    "spatie/dropbox-api": "^1.11"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "phpunit/phpunit": "^9",
    "phpmd/phpmd" : "@stable",
    "squizlabs/php_codesniffer": "3.*",
    "sebastian/phpcpd": "6.*"
  },
  "autoload": {
    "classmap": ["app/code/", "vendor/", "tests/"]
  }
}
